      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>name</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Status</Label>
                <PropertyName>status</PropertyName>
              </ListItem>
              <ListItem>
                <Label>State</Label>
                <PropertyName>state</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Model</Label>
                <PropertyName>modelName</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Firmware</Label>
                <PropertyName>firmwareVersion</PropertyName>
              </ListItem>
              <ListItem>
                <Label>vPCRole</Label>
                <ScriptBlock>'{0}' -f $_.logicalSwitchVPC.role</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>vPCPeer</Label>
                <ScriptBlock>'{0}' -f $_.logicalSwitchVPC.peer.name</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>LogicalSwitch</Label>
                <ScriptBlock>'{0}' -f (Send-HPOVRequest $_.logicalSwitchUri -Hostname $_.ApplianceConnection.Name).name</ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
